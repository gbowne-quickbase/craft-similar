{
    "name": "nystudio107/craft-similar",
    "description": "Similar for Craft lets you find elements, Entries, Categories, Commerce Products, etc, that are similar, based on... other related elements.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "elements",
        "relations",
        "similar"
    ],
    "support": {
        "docs": "https://github.com/nystudio107/craft-similar/blob/v1/README.md",
        "issues": "https://github.com/nystudio107/craft-similar/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "nystudio107.com",
            "homepage": "https://nystudio107.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "nystudio107\\similar\\": "src/"
        }
    },
    "extra": {
        "name": "Similar",
        "handle": "similar",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/nystudio107/craft-similar/v1/CHANGELOG.md",
        "components": {
            "similar": "nystudio107\\similar\\services\\Similar"
        },
        "class": "nystudio107\\similar\\Similar"
    }
}
